{
  "name": "gatsby-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "Gatsby boilerplate with TypeScript, Tailwind CSS, PostCSS, ESLint, and Prettier",
  "author": "Devxhub Ltd.",
  "keywords": [
    "gatsby"
